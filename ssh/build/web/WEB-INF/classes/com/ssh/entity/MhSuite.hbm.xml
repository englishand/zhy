<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

<class 
    name="com.ssh.entity.MhSuite" 
    table="MH_SUITES"
>

    <id
        name="fsuitecode"
        type="java.lang.String"
        column="F_SUITECODE"
    >
        <generator class="assigned" />
    </id>

    <property
        name="fsuitename"
        type="java.lang.String"
        column="F_SUITENAME"
        length="100"
        access="field"
    />
    <property
        name="fsuitpath"
        type="java.lang.String"
        column="F_SUITPATH"
        length="255"
    />
    <property
        name="fstateflag"
        type="java.lang.String"
        column="F_STATEFLAG"
        not-null="true"
        length="1"
        access="field"
    />
    <property
        name="fdeleteflag"
        type="java.lang.String"
        column="F_DELETEFLAG"
        not-null="true"
        length="1"
    />
    <property
        name="fportalcode"
        type="java.lang.String"
        column="F_PORTALCODE"
        length="4"
    />
    
</class>
</hibernate-mapping>
